<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

            <ImageView
                android:id="@+id/instagram_logo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/instagram_logo"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ic_share"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_share"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/ic_heart"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_heart"
                app:layout_constraintEnd_toStartOf="@id/ic_share"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/user_story"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/user_story"
                app:layout_constraintTop_toBottomOf="@id/instagram_logo"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageView
                android:id="@+id/asset_story"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/asset_story"
                app:layout_constraintTop_toBottomOf="@id/instagram_logo"
                app:layout_constraintStart_toEndOf="@id/user_story" />

            <ImageView
                android:id="@+id/liga_story"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/liga_story"
                app:layout_constraintTop_toBottomOf="@id/instagram_logo"
                app:layout_constraintStart_toEndOf="@id/asset_story" />

            <ImageView
                android:id="@+id/asset_story_1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/asset_story"
                app:layout_constraintTop_toBottomOf="@id/instagram_logo"
                app:layout_constraintStart_toEndOf="@id/liga_story" />

            <ImageView
                android:id="@+id/liga_story_1"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:src="@drawable/liga_story"
                app:layout_constraintTop_toBottomOf="@id/instagram_logo"
                app:layout_constraintStart_toEndOf="@id/asset_story_1" />

            <TextView
                android:id="@+id/cerita_anda"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/cerita_anda"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/user_story"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/assets"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/assets"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/asset_story"
                app:layout_constraintStart_toEndOf="@id/cerita_anda"/>

            <TextView
                android:id="@+id/liga"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/liga"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/liga_story"
                app:layout_constraintStart_toEndOf="@id/assets"/>

            <TextView
                android:id="@+id/assets_1"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/assets"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/asset_story"
                app:layout_constraintStart_toEndOf="@id/liga"/>

            <TextView
                android:id="@+id/liga_1"
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/liga"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/asset_story"
                app:layout_constraintStart_toEndOf="@id/assets_1"/>

            <ImageView
                android:id="@+id/fufufafa_pp"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/fufufafa_pp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="15dp"
                app:layout_constraintTop_toBottomOf="@id/cerita_anda"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/fufufafa"
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="@string/fufufafa"
                android:textSize="11sp"
                android:textStyle="bold"
                android:textAlignment="center"
                android:layout_marginTop="25dp"
                app:layout_constraintTop_toBottomOf="@id/cerita_anda"
                app:layout_constraintStart_toEndOf="@id/fufufafa_pp"/>

            <TextView
                android:id="@+id/putra_mulyono"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="putra Mulyono"
                android:textSize="11sp"
                android:textAlignment="center"
                android:layout_marginTop="40dp"
                app:layout_constraintTop_toBottomOf="@id/cerita_anda"
                app:layout_constraintStart_toEndOf="@id/fufufafa_pp"/>

            <ImageView
                android:id="@+id/titik_tiga"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_titik_tiga"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintTop_toBottomOf="@id/cerita_anda"
                app:layout_constraintEnd_toEndOf="parent"/>

            <ImageView
                android:id="@+id/fufufafa_post"
                android:layout_width="match_parent"
                android:layout_height="360dp"
                android:layout_marginTop="10dp"
                android:src="@drawable/post_fufufafa"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_pp" />

            <ImageView
                android:id="@+id/fufufafa_heart"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/ic_heart"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_post" />

            <ImageView
                android:id="@+id/fufufafa_comment"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/ic_comment"
                app:layout_constraintStart_toEndOf="@id/fufufafa_heart"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_post" />

            <ImageView
                android:id="@+id/fufufafa_share"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginStart="5dp"
                android:src="@drawable/ic_share"
                app:layout_constraintStart_toEndOf="@id/fufufafa_comment"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_post" />

            <ImageView
                android:id="@+id/fufufafa_bookmark"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_marginEnd="5dp"
                android:src="@drawable/ic_bookmark"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_post" />

            <TextView
                android:id="@+id/disukai"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="Disukai oleh Ikhwan_Hanif"
                android:textSize="11sp"
                android:textAlignment="center"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_heart"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/fufufafa_comment_1"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="Fufufafa"
                android:textSize="11sp"
                android:textAlignment="center"
                android:textStyle="bold"
                app:layout_constraintTop_toBottomOf="@id/disukai"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/fufufafa_comment_2"
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:text="#Proud"
                android:textSize="11sp"
                android:textAlignment="center"
                android:textStyle="bold"
                android:textColor="#03b1fc"
                app:layout_constraintTop_toBottomOf="@id/disukai"
                app:layout_constraintStart_toEndOf="@id/fufufafa_comment_1"/>

            <TextView
                android:id="@+id/tampilkan"
                android:layout_width="170dp"
                android:layout_height="wrap_content"
                android:text="tampilkan 10 komentar lainnya"
                android:textSize="11sp"
                android:textAlignment="center"
                app:layout_constraintTop_toBottomOf="@id/fufufafa_comment_1"
                app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/assets_ugm_pp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/assets_ugm_pp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="15dp"
        app:layout_constraintTop_toBottomOf="@id/tampilkan"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/assets_ugm"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="assets_ugm"
        android:textSize="11sp"
        android:textStyle="bold"
        android:textAlignment="center"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/tampilkan"
        app:layout_constraintStart_toEndOf="@id/assets_ugm_pp"/>

    <TextView
        android:id="@+id/sv_ugm"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:text="Sekolah Vokasi Universitas Gadjah Mada"
        android:textSize="11sp"
        android:textAlignment="center"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@id/tampilkan"
        app:layout_constraintStart_toEndOf="@id/fufufafa_pp"/>

    <ImageView
        android:id="@+id/titik_tiga_1"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/ic_titik_tiga"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintTop_toBottomOf="@id/tampilkan"
        app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
        android:id="@+id/ic_home_active"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_home_active"
        android:layout_marginTop="20dp"
        android:layout_marginStart="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/ic_search"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_search"
        android:layout_marginTop="20dp"
        android:layout_marginStart="90dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/ic_upload"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_upload"
        android:layout_marginTop="20dp"
        android:layout_marginStart="180dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/ic_reels"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_reels"
        android:layout_marginTop="20dp"
        android:layout_marginStart="270dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btn_ic_profile"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_profile"
        android:layout_marginTop="20dp"
        android:layout_marginStart="360dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
